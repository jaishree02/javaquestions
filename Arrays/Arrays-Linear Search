import java.util.*;
public class Main {
    public static void main(String args[]) {
        // Your Code Here
        Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int arr[]=new int[n];
		for(int i=0;i<arr.length;i++) {
			arr[i]=sc.nextInt();
		}
		int m=sc.nextInt();
		boolean found=false;
		for(int i=0;i<arr.length;i++) {
			if(arr[i]==m) {
				System.out.println(i);
				found=true;
				break;
			}
			
		}
		if(!found) {
			System.out.println("-1");
		}


    }
}
