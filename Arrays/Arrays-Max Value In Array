import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int arr[]=new int[n];
		for(int i=0;i<arr.length;i++) {
			arr[i]=sc.nextInt();
		}
		int maxvalue=arr[0];
		for(int i=0;i<arr.length;i++) {
			if(arr[i]>maxvalue)
			{
				maxvalue=arr[i];
			}
		}
		System.out.println(maxvalue);



    }
}
